#. type: Plain text
#: index.md:11
#, fuzzy, no-wrap
msgid " _rf_GPIO_ ebligas simuli rflink kun dissendilo kaj ricevilo konektita rekte al la havenoj _GPIO_ de a _raspberry pi_.\n"
msgstr " _rf_GPO_ permite simular RFLINK con un transmisor y un receptor conectado directamente a los puertos _GPIO_ de A _raspberry pi_.\n"

#. type: Title ##
#: index.md:13
#, fuzzy, no-wrap
msgid "Instalado:"
msgstr "Instalación:"

#. type: Fenced code block
#: index.md:15
#, no-wrap
msgid ""
"cd /home/pi\n"
"git clone https://github.com/jmichault/rf_gpio.git\n"
"cd rf_gpio  \n"
"sudo cp -p rf_gpio.sh /etc/init.d  \n"
"sudo update-rc.d rf_gpio.sh defaults  \n"
"sudo service rf_gpio.sh start  \n"
msgstr ""
"cd /home/pi\n"
"git clone https://github.com/jmichault/rf_gpio.git\n"
"cd rf_gpio  \n"
"sudo cp -p rf_gpio.sh /etc/init.d  \n"
"sudo update-rc.d rf_gpio.sh defaults  \n"
"sudo service rf_gpio.sh start  \n"

